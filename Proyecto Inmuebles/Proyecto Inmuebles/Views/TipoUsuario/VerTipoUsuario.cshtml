@model Proyecto_Inmuebles.Models.TipoUsuarioVerViewModel
@{
    ViewData["Title"] = "Ver Tipo de Usuario";
}

<h2>@ViewData["Title"]</h2>
<p style="margin-top:12px;">
    

    <form asp-action="" method="post" style="display:grid; gap:8px; max-width:280px;">
        <label>IdTipoUsuario</label>
        <input asp-for="IdTipoUsuario" disabled/>

        <label>NombreTipo</label>
        <input asp-for="NombreTipo" disabled />

    </form>

</p>

<a asp-controller="TipoUsuario" asp-action="Index" class="btn btn-outline-info">@("<- Regresar")</a>
