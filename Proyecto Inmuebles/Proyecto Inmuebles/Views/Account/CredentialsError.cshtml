@{
    ViewData["Title"] = "Error de credenciales";
    var returnUrl = (string?)ViewData["ReturnUrl"] ?? "/";
}

<h2>Credenciales invalidas</h2>
<p>El usuario o contrasena son incorrectos.</p>

<p style="margin-top:12px;">
    <a asp-controller="Account" asp-action="Login" asp-route-returnUrl="@returnUrl">Regresar al login</a>
</p>